<%# content_for :title, 'About' %>
<% @pages.each do |page| %>
<div class="box-block">
  <div class="row post-header">
    <div class="col-xs-12 col-md-12 subtitle text-center">
      <%= page.title %>
    </div>
  </div>
  <div class="row">
    <div class="col-xs-12 col-md-12"><%= raw page.text %></div>
  </div>
</div>
<% end %>